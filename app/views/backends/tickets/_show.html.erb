<tr id="ticket<%= ticket.id %>">
<td>
<%=ticket.id%>
</td>
  <td> 
    <% if ticket.cover %>
    <img class="img-responsive" src="<%= ticket.cover %>">
    <% end %>
  </td>
  <td>
    <%= ticket.name %>
  </td>
  <td>
    <%= link_to(edit_backends_ticket_path(ticket),class: "btn btn-xs btn-warning") do %>
    <i class="fa fa-pencil"></i>
    <% end %>   
    <%= link_to(backends_ticket_path(ticket),class: "btn btn-xs btn-danger", format: :js, remote: true, method: :delete,data:{confirm: "確認刪除票券?"}) do %>
    <i class="fa fa-trash-o"></i>
    <% end %>     
  </td>  
</tr>