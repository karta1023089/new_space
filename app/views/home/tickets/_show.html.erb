<div class="list-item-entry">
    <div class="hotel-item style-10 bg-white">
      <div class="table-view">
          <div class="radius-top cell-view">
            <div class="img-box f-img" style="background-image: url(<%=ticket.cover%>);"></div>                
          </div>
          <div class="title hotel-middle cell-view">
            <h5>單趟 <strong class="color-red-3 m-icon"><%=ticket.price%></strong> 起</h5>
            <h6 class="color-grey-3 list-hidden">one way flights</h6>
              <h4><b><%=ticket.intro%></b>&nbsp;&nbsp;<%=ticket.name%></h4>
              
              <div class="fi_block grid-hidden row row10">
                <div class="flight-icon col-xs-6 col10">
                  <img class="fi_icon" src="/img/tour_list/flight_icon_2.png" alt="">
                  <div class="fi_content">
                    <div class="fi_title color-dark-2"><%=ticket.article1%></div>
                    <div class="fi_text color-grey"><%=ticket.go_date%></div>
                  </div>
                </div>
                <div class="flight-icon col-xs-6 col10">
                  <img class="fi_icon" src="/img/tour_list/flight_icon_1.png" alt="">
                  <div class="fi_content">
                    <div class="fi_title color-dark-2"><%=ticket.article2%></div>
                    <div class="fi_text color-grey"><%=ticket.back_date%></div>
                  </div>
                </div>
              </div>
            <a target="_blank" href="<%=ticket.url%>" class="c-button b-40 bg-red-3 hv-red-3-o">了解更多</a>
          </div>
                              
        </div>
    </div>          
</div>