<div class="row">
  <div class="col-lg-6">
    <section class="panel">
      <header class="panel-heading">
        <%= Admin.model_name.human %>
      </header>
      <div class="panel-body">
        <form action="<%= admin_path %>" method="post" class="form-horizontal" > 
          <%= token_tag nil %>
